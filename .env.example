# Environment Variables Example
# Copy this file to .master-env.development and .master-env.production
# Fill in the actual values for your environment

# ========= [api] =========
# Database
DATABASE_TYPE=mongodb
DATABASE_URI=your_database_connection_string
PAYLOAD_SECRET=your_payload_secret_key
PORT=8000
SEED_DATABASE=true

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
NEXT_PUBLIC_WEB_URL=http://localhost:8001

# ========= [db] =========
# MongoDB (for development)
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=password
MONGO_DATABASE=mongodb

# PostgreSQL (for production)
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_postgres_database

# ========= [web] =========
PORT=8001
NEXT_PUBLIC_ASSETS_DIR=
NEXT_PUBLIC_API_URL=http://localhost:8000

# Mapbox
NEXT_PUBLIC_MAPBOX_API_KEY=your_mapbox_api_key

# AWS Amplify
NEXT_PUBLIC_AWS_AMPLIFY_USER_POOL_ID=your_amplify_user_pool_id
NEXT_PUBLIC_AWS_AMPLIFY_USER_POOL_WEB_CLIENT_ID=your_amplify_web_client_id
NEXT_PUBLIC_AWS_AMPLIFY_REGION=your_aws_region

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# ========= [web-demo] =========
PORT=8002
NEXT_PUBLIC_ASSETS_DIR=
NEXT_PUBLIC_API_URL=http://localhost:8000

# Mapbox
NEXT_PUBLIC_MAPBOX_API_KEY=your_mapbox_api_key

# AWS Amplify
NEXT_PUBLIC_AWS_AMPLIFY_USER_POOL_ID=your_amplify_user_pool_id
NEXT_PUBLIC_AWS_AMPLIFY_USER_POOL_WEB_CLIENT_ID=your_amplify_web_client_id
NEXT_PUBLIC_AWS_AMPLIFY_REGION=your_aws_region

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key