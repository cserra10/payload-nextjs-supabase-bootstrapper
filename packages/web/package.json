{
  "name": "web",
  "author": "Cesar Serrano",
  "version": "7.3.0",
  "description": "Next Starter & TypeScript",
  "private": true,
  "scripts": {
    "predev": "cp .env.development .env",
    "dev": "dotenv -e .env.development -- sh -c 'next dev -p $PORT'",
    "prestart": "cp .env.production .env",
    "start": "dotenv -e .env.production -- sh -c 'next start -p $PORT'",
    "build": "next build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules .next out dist build",
    "re:dev": "yarn clean && yarn install && yarn dev",
    "re:build": "yarn clean && yarn install && yarn build",
    "re:build-npm": "npm run clean && npm install && npm run build",
    "tsc:dev": "yarn dev & yarn tsc:watch",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:print": "npx tsc --showConfig"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.4.0",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/dm-sans": "^5.2.6",
    "@fontsource-variable/inter": "^5.2.6",
    "@fontsource-variable/nunito-sans": "^5.2.6",
    "@fontsource-variable/public-sans": "^5.2.6",
    "@fontsource/barlow": "^5.2.6",
    "@hookform/resolvers": "^5.1.1",
    "@iconify/react": "^6.0.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/stylis-plugin-rtl": "^7.1.0",
    "@mui/x-data-grid": "^8.5.3",
    "@mui/x-date-pickers": "^8.5.3",
    "@mui/x-tree-view": "^8.5.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.49.8",
    "accept-language": "^3.0.20",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.39.3",
    "framer-motion": "^12.18.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "minimal-shared": "^1.0.15",
    "next": "^15.3.4",
    "nprogress": "^0.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.58.1",
    "react-i18next": "^15.5.3",
    "simplebar-react": "^3.3.0",
    "sonner": "^2.0.5",
    "stylis": "^4.3.6",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@svgr/webpack": "^8.1.0",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/node": "^24.0.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/stylis": "^4.2.7",
    "@typescript-eslint/parser": "^8.34.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
